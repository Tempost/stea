{
  "compilerOptions": {
    "target": "es5",
    "resolveJsonModule": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
